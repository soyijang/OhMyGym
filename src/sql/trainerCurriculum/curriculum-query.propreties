selectTrainerCurriculum=SELECT * FROM TRAINER_CURRICULUM WHERE MEMBER_ID = ?
selectTrainerVideo=SELECT V.* FROM TRAINER_VIDEO V JOIN TRAINER_CURRICULUM C ON(C.CURRICULUM_CODE = V.CURRICULUM_CODE) WHERE C.MEMBER_ID = ? AND V.CURRICULUM_CODE = 1 ORDER BY V.TRAINER_UPLOADDATE DESC, V.TRAINER_UPLOADTIME DESC
selectCurriTitle=SELECT CURRICULUM_TITLE FROM TRAINER_CURRICULUM WHERE CURRICULUM_CODE = ?